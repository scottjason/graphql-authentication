{
  "name": "react-graphql-authentication",
  "version": "1.0.0",
  "description": "Authentication boilerplate",
  "author": "Scott Jason",
  "license": "ISC",
  "engines": {
    "node": "14.x",
    "npm": "^6 || ^8"
  },
  "keywords": [
    "react",
    "node",
    "apollo",
    "graphql",
    "mongoDB",
    "express",
    "webpack",
    "typescript",
    "authentication"
  ],
  "scripts": {
    "start": "node dist/server.bundle.js",
    "postinstall": "run-s build:client build:server",
    "launch-browser": "open http://localhost:3000",
    "start-dev": "run-p start:server start:client",
    "start:client": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.config.js --progress",
    "start:server": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "build": "run-s build:client build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/webpack.server.config.js --progress",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm dedupe && lint-staged && pretty-quick --staged"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "animate.css": "^4.1.1",
    "apollo-server": "^3.5.0",
    "apollo-server-express": "^3.6.1",
    "bcryptjs": "^2.4.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.5.1",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.20.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.14",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.22.5",
    "react-icons": "^4.3.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.23.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.19",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "find-config": "^1.0.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack-cli": "^4.9.0",
    "webpack-dev-middleware": "^5.3.0",
    "webpack-dev-server": "^4.7.3"
  }
}
